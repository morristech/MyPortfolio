<?xml version="1.0" encoding="utf-8"?><android.support.design.widget.CoordinatorLayout	xmlns:android="http://schemas.android.com/apk/res/android"	xmlns:app="http://schemas.android.com/apk/res-auto"	android:id="@+id/activity_coding_cl_main_layout"	android:layout_width="match_parent"	android:layout_height="match_parent"	>	<android.support.design.widget.AppBarLayout		android:id="@+id/activity_coding_appbar"		android:layout_width="match_parent"		android:layout_height="wrap_content"		app:contentInsetStart="0dp"		>		<android.support.design.widget.CollapsingToolbarLayout			android:id="@+id/activity_coding_ctl"			android:layout_width="match_parent"			android:layout_height="256dp"			android:background="@drawable/about_me_background"			app:contentScrim="@color/primary_color"			app:layout_scrollFlags="scroll|exitUntilCollapsed"			app:elevation="4dp">			<View				android:layout_width="match_parent"				android:layout_height="match_parent"				android:background="@drawable/music_transparent_gradient"				app:layout_collapseMode="parallax"/>			<android.support.v7.widget.Toolbar				android:id="@+id/activity_coding_toolbar"				android:layout_width="match_parent"				android:layout_height="?attr/actionBarSize"				app:contentInsetStart="0dp"				app:layout_collapseMode="pin"				app:title="">				<TextView					android:layout_width="wrap_content"					android:layout_height="match_parent"					android:gravity="center_vertical"					android:text="@string/app_name"					android:textAppearance="@style/TextAppearance.LobsterBold"					android:textColor="@android:color/white"					android:textSize="20sp"/>			</android.support.v7.widget.Toolbar>			<com.panwrona.myportfolio.customviews.skills_level_view.SkillsLevelView				android:id="@+id/skills_level_view"				android:layout_width="180dp"				android:layout_height="120dp"				android:layout_gravity="center"				app:radius="6dp"				app:skillScale="7"/>		</android.support.design.widget.CollapsingToolbarLayout>		<android.support.design.widget.TabLayout			android:id="@+id/activity_coding_tl_container"			android:layout_width="match_parent"			android:layout_height="wrap_content"			android:layout_gravity="bottom"			app:tabGravity="fill"			app:tabIndicatorColor="@color/accent_color"			app:tabSelectedTextColor="@android:color/white"			app:tabTextAppearance="@style/MyTabLayoutTextAppearance"			app:tabTextColor="@color/light_primary_color"			app:tabBackground="@color/primary_color"/>	</android.support.design.widget.AppBarLayout>	<android.support.v4.view.ViewPager		android:id="@+id/activity_coding_vp_container"		android:layout_width="match_parent"		android:layout_height="match_parent"	app:layout_behavior="@string/appbar_scrolling_view_behavior"	android:background="@android:color/white"/>	<!--<android.support.v4.widget.NestedScrollView-->		<!--android:id="@+id/activity_coding_nsv_container"-->		<!--android:layout_width="match_parent"-->		<!--android:layout_height="match_parent"-->		<!--app:layout_behavior="@string/appbar_scrolling_view_behavior"-->		<!--app:behavior_overlapTop="64dp">-->		<!--<android.support.v7.widget.CardView-->			<!--android:id="@+id/activity_coding_cv_container"-->			<!--android:layout_width="match_parent"-->			<!--android:layout_height="match_parent"-->			<!--android:background="@android:color/white"-->			<!--android:layout_margin="16dp"-->			<!--android:visibility="invisible"-->			<!--android:layout_marginBottom="36dp"-->			<!--app:cardElevation="8dp">-->			<!--<LinearLayout-->				<!--android:id="@+id/activity_coding_ll_container"-->				<!--android:layout_width="match_parent"-->				<!--android:layout_height="wrap_content"-->				<!--app:layout_anchor="@id/activity_coding_ctl"-->				<!--app:layout_anchorGravity="bottom"-->				<!--android:orientation="vertical">-->				<!--<com.panwrona.myportfolio.customviews.support_layouts.WrapContentHeightViewPager-->					<!--android:id="@+id/activity_coding_vp_container